{
  "hostedProcess": {
    "healthCheck": {
      "port": 5000,
      "protocol": "http",
      "processTimeoutSeconds": 60
    }
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": true,
    "variables": {
      "toconsole": "${longdate}|${level:uppercase=true}|${threadid}|${logger:shortName=true}|${message}${onexception:inner=${exception:format=ToString:innerFormat=StackTrace:maxInnerExceptionLevel=5}|${all-event-properties}"
    },
    "targets": {
      "sendConsole": {
        "type": "Console",
        "layout": "${toconsole}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "sendConsole",
        "final": false
      }
    ]
  }
}
